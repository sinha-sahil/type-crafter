{{#each values}}
  | {{#if (eq ../type 'string') }}'{{/if}}{{{this}}}{{#if (eq ../type 'string')}}'{{/if}}
{{/each}};

export function decode{{typeName}}(rawInput: unknown): {{typeName}} | null {
